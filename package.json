{
  "name": "maoflow",
  "version": "1.0.0",
  "description": "一个基于 Electron + FastAPI + React 的跨平台应用",
  "main": "out/main/index.js",
  "private": true,
  "scripts": {
    "dev": "electron-vite dev",
    "dev:web": "cd web-ui && vite",
    "dev:api": "cd backend && python -m uvicorn app.main:app --reload",
    "build": "source $(conda info --base)/etc/profile.d/conda.sh && conda activate MaoFlow && electron-vite build",
    "build:web": "cd web-ui && vite build",
    "build:electron": "source $(conda info --base)/etc/profile.d/conda.sh && conda activate MaoFlow && electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ant-design/x": "^1.0.5",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "antd": "^5.24.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
